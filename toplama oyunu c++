#include <iostream>
using namespace std;

int main()
{
	int a[5];
	cout << "Lutfen 5 sayi giriniz" << endl;
	for (int i = 0; i < 5; i++) {
		cin >> a[i];
	}
	int eb = a[0];
	int ek = a[0];
	int top = 0;
	for (int i = 0; i < 5; i++) {
		top += a[i];
		if (a[i] > eb)
			eb = a[i];
		if (a[i] < ek)
			ek = a[i];
	}
	cout << "maks:" << top - ek << endl;
	cout << "min" << top - eb << endl;
} 
